<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
    integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />
  <link href="https://fonts.googleapis.com/css2?family=Smokum&display=swap" rel="stylesheet">
  <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Merriweather:wght@300&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="css/style.css">
</head>

<body>
  <!-- header Start-->
  <%- include('../partials/header.ejs') %>
  <!--header end-->

  <!--Banner start-->
  <section id="banner">
    <div class="container">
      <div class="row">
        <div class="col-md-8">
          <p id="banner-heading"><%= banner.title%></p>
          <p><%= banner.desc%></p>
            <p><a href="/banner"><button class="btn btn-success">More Deatils</button></a></p>
        </div>
        <div class="col-md-4"><span><img src="upload/<%=banner.img%>" alt="" class="img-fluid d-block mx-auto" id="banner-img"></div>
      </div>
    </div>
    <img src="media/border1.png" alt="" class="banner-border-img">
  </section>
  <!--Banner end-->



 <!--Testi start-->

 <section id="testi" class="bg-light">
    <div class="container pb-5">
          <p id="heading">Testinominals</p>
      <div class="row text-center">
        <%testiREcords.forEach((result)=>{%>
          <div class="col-md-3">
            <img src="media/building.png" class="img-fluid" alt="">
            <p id="desc"><%=result.quotes%></p>
            <p id="company"><span id="company-name">Company Name:</span><%=result.companyname%></p>
          
          </div>
        <%})%>
        
      </div>
    </div>
 </section>
  <!--testi end-->



  <!-- Footer Start-->
  <%- include('../partials/footer.ejs')%>
  <!--Footer end-->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"></script>
</body>

</html>